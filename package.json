    "@types/node": "18.14.1",
    "@types/supertest": "2.0.12",
    "@vitest/coverage-v8": "1.4.0",
    "@vitest/ui": "1.4.0",
    "eslint": "8.34.0",
    "npm-run-all": "4.1.5",
    "prisma": "4.10.1",
    "supertest": "6.3.3",
    "tsup": "8.0.2",
    "tsx": "4.7.1",
    "typescript": "4.9.5",
